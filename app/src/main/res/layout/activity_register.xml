<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register">


    <TextView
        android:id="@+id/lblusername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_name"
        app:layout_constraintEnd_toStartOf="@+id/txtUserName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="89dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserName"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#FFCEBED3"
        android:hint="hint"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="84dp" />


    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserSurname"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#CEBED3"
        android:hint="hint"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="125dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserPassword"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#FFCEBED3"
        android:hint="hint"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/lblPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_pwd"
        app:layout_constraintEnd_toStartOf="@+id/txtUserPassword"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="306dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserEps"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#FFCEBED3"
        android:hint="hint"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/lblEps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_Eps"
        app:layout_constraintEnd_toStartOf="@+id/txtUserEps"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="258dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserTel"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#FFCEBED3"
        android:hint="hint"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="344dp" />

    <TextView
        android:id="@+id/lblTel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_phone"
        app:layout_constraintEnd_toStartOf="@+id/txtUserTel"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="349dp" />

    <TextView
        android:id="@+id/lblUserId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_id"
        app:layout_constraintEnd_toStartOf="@+id/txtUserId"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="207dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/txtUserId"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:background="#FFCEBED3"
        android:hint="hint"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/lblSurname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/user_surname"
        app:layout_constraintEnd_toStartOf="@+id/txtUserSurname"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="130dp" />

    <TextView
        android:id="@+id/lblIdType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/user_idType"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="170dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/picture_profile"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="416dp" />

</androidx.constraintlayout.widget.ConstraintLayout>